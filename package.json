{
  "name": "bradtunes",
  "version": "1.0.3",
  "description": "Hi! Iâ€™m Brad, and this is BradTunes, an app that combines a full-featured music player with the power of yt-dlp downloads.",
  "main": "./out/main/index.js",
  "author": "Brad Moyetones",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BradMoyetones/BradTunes.git"
  },
  "homepage": "https://github.com/BradMoyetones/BradTunes#readme",
  "license": "MIT",
  "bugs": {
